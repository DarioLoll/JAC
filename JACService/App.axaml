<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="JACService.App"
             xmlns:local="using:JACService"
             xmlns:models="clr-namespace:JACService.Models"
             xmlns:styling="clr-namespace:FluentAvalonia.Styling;assembly=FluentAvalonia"
             RequestedThemeVariant="Dark">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
             
             <Application.Styles>
                 <styling:FluentAvaloniaTheme />
             </Application.Styles>
             
             <Application.Resources>
                 <ResourceDictionary>
                     <Color x:Key="InfoColor">#31a4ff</Color>
                     <SolidColorBrush x:Key="WarningColor" Color="#ffb34d" />
                     <SolidColorBrush x:Key="ErrorColor" Color="#fd694f" />
                     <SolidColorBrush x:Key="SuccessColor" Color="#7dce83" />
                 </ResourceDictionary>
             </Application.Resources>
</Application>